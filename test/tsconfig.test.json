{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "paths": {
      "react/index": ["node_modules/@types/react/index"],
      "react": ["../jsx/react"],
      "react/jsx-runtime": ["../jsx/jsx-runtime"],
      "react/jsx-dev-runtime": ["../jsx/jsx-dev-runtime"]
    },
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "incremental": true,
    "disableSizeLimit": true,
    "allowSyntheticDefaultImports": true
  }
}
